
@using System.Web.Optimization
@model PostaFlya.Models.Flier.BulletinDetailPageModel

@{
    ViewBag.Title = "DetailGet";
    Layout = "~/Views/Shared/_DefaultLayout.cshtml";
}

@section title
{
    @if (Model.Detail != null)
    {
        @:PostaFlya - @Model.Detail.Flier.Title

    }
    else
    {
        @:Postaflya - Your Local Community Noticeboard
    } 

}

@section scripts
{
    @Scripts.Render("~/Script/BulletinDeskJs.js")
}

@Html.Partial("Bulletin/DetailTileTemplates")

@Html.Partial("DetailViewPartial", Model.Detail)
