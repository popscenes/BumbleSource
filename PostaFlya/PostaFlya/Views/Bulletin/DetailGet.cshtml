
@using System.Web.Optimization
@model dynamic

@{
    ViewBag.Title = "DetailGet";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section title
{
    @if (Model != null)
    {
        @:PostaFlya - @Model.Flier.Title

    }
    else
    {
        @:Postaflya - Your Local Community Noticeboard
    } 

}

@section scripts {
    @Styles.Render("~/Content/structure/Bulletin/DeskCss.css")   
    @Styles.Render("~/Content/themes/taskflya/Bulletin/DeskCss.css")   
    @Scripts.Render("~/Script/BulletinDetDeskJs.js") 
}

@Html.Partial("Bulletin/DetailTileTemplates")

@Html.Partial("DetailViewPartial")
