

<div class="imageSelector">
    <div data-bind="attr:{id:uploaderElementId}">
       
    </div>
    
    <ul data-bind="template: { name: 'image-template', foreach: imageList}, attr:{id:imageListId}">
        
    </ul>
    
    <button data-bind="click: toggleAddImage" class="Button">add image</button>

    
    <script type="text/html" id="image-template">
        <li style="width:120px; margin: 10px; padding: 0px;">
            <img data-bind="attr: { id: ImageId, src: ImageUrl  }, click: $parent.clickImage, imgRetry: ImageUrl" height="100" />
        </li>
    </script>
    
</div>